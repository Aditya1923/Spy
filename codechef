import java.util.*;
public class Spy{
public static void main(String args[]){
Scanner sc=new Scanner(System.in);
int a,r=0,s=0,s1=1;
System.out.println("Enter a number");
a=sc.nextInt();
while(a!=0){
r%=10;
s+=r;
s1*=r;
a/=10;
}
if(s1==s)
System.out.println("Spy number");
else
System.out.println("Not a Spy number");
}
}
